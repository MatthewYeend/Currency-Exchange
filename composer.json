{
    "name": "mattyeend/currency-exchange",
    "description": "A Laravel package for currency exchange rates",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "Laravel",
        "Prevent",
        "Destructive",
        "Commands",
        "Prevent-Destructive-Commands",
        "Package"
    ],
    "authors": [
        {
            "name": "Matt Yeend",
            "email": "mail@mattyeend.co.uk"
        }
    ],
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^10.0 || ^11.0 || ^12.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "MattYeend\\CurrencyExchange\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MattYeend\\CurrencyExchange\\CurrencyExchangeServiceProvider"
            ]
        }
    }
}